
public class ContaCorrente extends Conta{

	private double saldo;
	
	public double getSaldo() {
		return saldo;
	}

	public void setSaldo(double saldo) {
		this.saldo = saldo;
	}

	public void atualiza(double valor) {
		this.saldo += valor;

	}

	public void deposita(double valor) {
		this.saldo += valor;
	}

}
