
public abstract class Funcionario {

	private String nome;
	private String cpf;
	protected double salario;

	public String getNome() {
		return nome;
	}

	public String getCpf() {
		return cpf;
	}

	public void setCpf(String cpf) {
		this.cpf = cpf;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	abstract double getBonificacao();
	
	public void mostra() {
		this.getBonificacao();
		
	}

}
